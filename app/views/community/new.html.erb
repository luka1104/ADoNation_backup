<div class="posts-main posts-new">
  <div class="posts-container">
    <h1 class="form-heading">投稿する</h1>
    <%= form_tag("/community/create") do %>
      <div class="form">
        <div class="form-body">
          <textarea name="content"><%= @post.content %></textarea>
          <input type="submit" value="投稿">
        </div>
      </div>
    <% end %>
  </div>
</div>