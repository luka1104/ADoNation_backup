<div class="main projects-edit">
  <div class="edit-container">
    <h1 class="form-heading">編集する</h1>
    <%= form_tag("/projects/#{@project.id}/update") do %>
    <div class="form">
      <div class="form-body">
        <textarea name="content"><%= @project.content %></textarea>
        <input type="submit" value="保存">
      </div>
    </div>
    <% end %>
  </div>
</div>